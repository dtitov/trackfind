    "properties": {
      "non_standard_samples": {
        "$id": "#/items/properties/non_standard_samples",
        "type": "object",
        "title": "The Non_standard_samples Schema",
        "required": [
          "local_id",
          "global_id",
          "phenotype",
          "sample_type",
          "tissue_type",
          "biomaterial_type"
        ],
        "properties": {
          "local_id": {
            "$id": "#/items/properties/non_standard_samples/properties/local_id",
            "type": "string",
            "title": "The Local_id Schema",
            "default": "",
            "examples": [
              "GEO:GSM945229"
            ],
            "pattern": "^(.*)$"
          },
          "global_id": {
            "$id": "#/items/properties/non_standard_samples/properties/global_id",
            "type": "string",
            "title": "The Global_id Schema",
            "default": "",
            "examples": [
              "biosample:SAMN01731491"
            ],
            "pattern": "^(.*)$"
          },
          "phenotype": {
            "$id": "#/items/properties/non_standard_samples/properties/phenotype",
            "type": "object",
            "title": "The Phenotype Schema",
            "required": [
              "term_url",
              "term_value"
            ],
            "properties": {
              "term_url": {
                "$id": "#/items/properties/non_standard_samples/properties/phenotype/properties/term_url",
                "type": "string",
                "title": "The Term_url Schema",
                "default": "",
                "examples": [
                  "http://purl.obolibrary.org/obo/PATO_0000461"
                ],
                "pattern": "^(.*)$"
              },
              "term_value": {
                "$id": "#/items/properties/non_standard_samples/properties/phenotype/properties/term_value",
                "type": "string",
                "title": "The Term_value Schema",
                "default": "",
                "examples": [
                  "normal"
                ],
                "pattern": "^(.*)$"
              }
            }
          },
          "sample_type": {
            "$id": "#/items/properties/non_standard_samples/properties/sample_type",
            "type": "object",
            "title": "The Sample_type Schema",
            "required": [
              "term_url",
              "term_value"
            ],
            "properties": {
              "term_url": {
                "$id": "#/items/properties/non_standard_samples/properties/sample_type/properties/term_url",
                "type": "string",
                "title": "The Term_url Schema",
                "default": "",
                "examples": [
                  "http://purl.obolibrary.org/obo/CL_0000236"
