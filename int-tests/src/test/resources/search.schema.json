              "e-mail": {
                "$id": "#/items/properties/studies/properties/contact/properties/e-mail",
                "type": "string",
                "title": "The E-mail Schema",
                "default": "",
                "examples": [
                  "encode-help@lists.stanford.edu"
                ],
                "pattern": "^(.*)$"
