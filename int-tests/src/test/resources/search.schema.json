              "orcid": {
                "$id": "#/items/properties/studies/properties/contact/properties/orcid",
                "type": "string",
                "title": "The Orcid Schema",
                "default": "",
                "examples": [
                  "0000-0000-0000-0000"
                ],
                "pattern": "^(.*)$"
