          "local_id": {
            "$id": "#/items/properties/experiments/properties/local_id",
            "type": "string",
            "title": "The Local_id Schema",
            "default": "",
            "examples": [
              "encode:ENCSR000DQP"
            ],
            "pattern": "^(.*)$"
          },
          "global_id": {
            "$id": "#/items/properties/experiments/properties/global_id",
            "type": "string",
            "title": "The Global_id Schema",
            "default": "",
            "examples": [
              "GEO:GSE35583"
