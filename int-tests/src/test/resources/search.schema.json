            "pattern": "^(.*)$"
          },
          "publication": {
            "$id": "#/items/properties/studies/properties/publication",
            "type": "string",
            "title": "The Publication Schema",
            "default": "",
            "examples": [
              "22955617"
            ],
            "pattern": "^(.*)$"
          }
