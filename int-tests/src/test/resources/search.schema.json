                "$id": "#/items/properties/non_standard_samples/properties/sample_type/properties/term_value",
                "type": "string",
                "title": "The Term_value Schema",
                "default": "",
                "examples": [
                  "H1-hESC"
                ],
                "pattern": "^(.*)$"
              }
            }
          },
          "tissue_type": {
            "$id": "#/items/properties/non_standard_samples/properties/tissue_type",
            "type": "object",
            "title": "The Tissue_type Schema",
            "required": [
              "term_url",
              "term_value"
            ],
            "properties": {
              "term_url": {
                "$id": "#/items/properties/non_standard_samples/properties/tissue_type/properties/term_url",
                "type": "string",
                "title": "The Term_url Schema",
                "default": "",
                "examples": [
                  "http://purl.obolibrary.org/obo/CL_0002322"
