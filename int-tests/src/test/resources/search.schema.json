          "label_short": {
            "$id": "#/items/properties/tracks/properties/label_short",
            "type": "string",
            "title": "The Label_short Schema",
            "default": "",
            "examples": [
              "ENCFF625ZYB"
            ],
            "pattern": "^(.*)$"
          },
          "content_type": {
            "$id": "#/items/properties/tracks/properties/content_type",
            "type": "object",
            "title": "The Content_type Schema",
            "required": [
              "term_url",
              "term_value"
            ],
            "properties": {
              "term_url": {
