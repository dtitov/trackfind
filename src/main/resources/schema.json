{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "fair"
  ],
  "properties": {
    "fair": {
      "$id": "#/properties/fair",
      "type": "object",
      "title": "The Fair Schema",
      "required": [
        "files"
      ],
      "properties": {
        "files": {
          "$id": "#/properties/fair/properties/files",
          "type": "array",
          "title": "The Files Schema",
          "items": {
            "$id": "#/properties/fair/properties/files/items",
            "type": "object",
            "title": "The Items Schema",
            "required": [
              "uri"
            ],
            "properties": {
              "uri": {
                "$id": "#/properties/fair/properties/files/items/properties/uri",
                "type": "string",
                "title": "The Uri Schema",
                "default": "",
                "examples": [
                  "uri"
                ],
                "pattern": "^(.*)$"
              },
              "data_type": {
                "$id": "#/properties/fair/properties/files/items/properties/data_type",
                "type": "string",
                "title": "The Data_type Schema",
                "default": "",
                "examples": [
                  "data_type"
                ],
                "pattern": "^(.*)$"
              },
              "file_suffix": {
                "$id": "#/properties/fair/properties/files/items/properties/file_suffix",
                "type": "string",
                "title": "The File_suffix Schema",
                "default": "",
                "examples": [
                  "file_suffix"
                ],
                "pattern": "^(.*)$"
              },
              "checksum": {
                "$id": "#/properties/fair/properties/files/items/properties/checksum",
                "type": "string",
                "title": "The Checksum Schema",
                "default": "",
                "examples": [
                  "checksum"
                ],
                "pattern": "^(.*)$"
              }
            }
          }
        },
        "genome_build": {
          "$id": "#/properties/fair/properties/genome_build",
          "type": "string",
          "title": "The Genome_build Schema",
          "default": "",
          "examples": [
            "genome_build"
          ],
          "pattern": "^(.*)$"
        },
        "tissue_type": {
          "$id": "#/properties/fair/properties/tissue_type",
          "type": "string",
          "title": "The Tissue_type Schema",
          "default": "",
          "examples": [
            "tissue_type"
          ],
          "pattern": "^(.*)$"
        },
        "cell_type": {
          "$id": "#/properties/fair/properties/cell_type",
          "type": "string",
          "title": "The Cell_type Schema",
          "default": "",
          "examples": [
            "cell_type"
          ],
          "pattern": "^(.*)$"
        },
        "target": {
          "$id": "#/properties/fair/properties/target",
          "type": "string",
          "title": "The Target Schema",
          "default": "",
          "examples": [
            "target"
          ],
          "pattern": "^(.*)$"
        },
        "experiment_type": {
          "$id": "#/properties/fair/properties/experiment_type",
          "type": "string",
          "title": "The Experiment_type Schema",
          "default": "",
          "examples": [
            "experiment_type"
          ],
          "pattern": "^(.*)$"
        },
        "data_source": {
          "$id": "#/properties/fair/properties/data_source",
          "type": "string",
          "title": "The Data_source Schema",
          "default": "",
          "examples": [
            "data_source"
          ],
          "pattern": "^(.*)$"
        },
        "nested_prop": {
          "$id": "#/properties/fair/properties/nested_prop",
          "type": "object",
          "title": "The Nested_prop Schema",
          "required": [
            "someField"
          ],
          "properties": {
            "someField": {
              "$id": "#/properties/fair/properties/nested_prop/properties/someField",
              "type": "string",
              "title": "The Somefield Schema",
              "default": "",
              "examples": [
                "someValue"
              ],
              "pattern": "^(.*)$"
            }
          }
        }
      }
    }
  }
}